{
  "expo": {
    "name": "OssApp",
    "slug": "OssApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoSinFondo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "marginHorizontal": 50,
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSinFondo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Logo.png"
    }
  }
}
